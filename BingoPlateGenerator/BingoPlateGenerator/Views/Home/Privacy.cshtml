@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>
@model BingoPlateGenerator.Models.DisplayModels.FactoryDisplay;

<p>Use this page to detail your site's privacy policy.</p>

@foreach (BingoPlate item in Model.MyDisplay.Batch)
{
    <table>
        <tbody>

            @for (int y = 0; y < 3; y++)
            {
                <tr>

                    @for (int x = 0; x < 9; x++)
                    {
                        <td>
                            @Html.DisplayFor(modelItem => item.Columns[y][x]);
                        </td>

                    }


                </tr>
            }

        </tbody>
    </table>
}